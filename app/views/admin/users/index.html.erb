<div class="col-lg-10 offset-md-1"><br><br><br><br><br><br>

  <h1>登録済みのユーザー一覧</h1>
  <table class="table">
    <thead>
      <tr>
        <th>編集1</th>
        <th>編集2</th>
        <th>id</th>
        <th>名前</th>
        <th>登録日</th>
        <th>メールアドレス</th>
        <th>電話番号</th>
      </tr>
    </thead>
    <tbody>
      <% @users.each do |user| %>
      <tr>
        <td><%= link_to '修正', edit_admin_user_path(user.id) %></td>
        <td><%= link_to "削除", "users/#{user.id}", method: :destroy, data: { confirm: '削除しますか？' } %></td>
        <td><%= user.id %></td>
        <td><%= user.name %></td>
        <td><%= user.mail %></td>
        <td><%= user.contact %></td>
        <td><%= user.created_at %></td>
      </tr>
      <% end %>
    </tbody>
  </table>
</div>